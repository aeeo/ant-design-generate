import request from '@/utils/request';
import { <%= moduleName %>Params, <%= moduleName %>Item } from './data.d';

export async function query<%= moduleName %>(params?: <%= moduleName %>Params) {
  return request('<%= serverURL.queryURL %>', {
    method: 'POST',
    data: params,
  });
}

export async function remove<%= moduleName %>(params: { id: number[] }) {
  return request('<%= serverURL.removeURL %>', {
    method: 'POST',
    data: {
      ...params,
    },
  });
}

export async function add<%= moduleName %>(params: <%= moduleName %>Item) {
  return request('<%= serverURL.addURL %>', {
    method: 'POST',
    data: {
      ...params,
    },
  });
}

export async function update<%= moduleName %>(params: <%= moduleName %>Item) {
  return request('<%= serverURL.updateURL %>', {
    method: 'POST',
    data: {
      ...params,
    },
  });
}
